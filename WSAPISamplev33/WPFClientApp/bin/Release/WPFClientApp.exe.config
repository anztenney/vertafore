						<transport clientCredentialType="None" proxyCredentialType="None" realm="">
              <!--<extendedProtectionPolicy policyEnforcement="Never" />--></transport>
						<message clientCredentialType="UserName" algorithmSuite="Default" />
					</security>
				</binding>
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="https://wsapi.ams360.com/v3/WSAPIservice.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_WSAPIServiceContract"
                contract="WSAPIService.WSAPIServiceContract" name="BasicHttpBinding_WSAPIServiceContract" />
		</client>
	</system.serviceModel>
